{
  "name": "electron-test",
  "productName": "electron-test",
    "version": "v1.0.7",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "lint": "echo \"No linting configured\"",
    "publish:osx": "./node_modules/electron-packager/bin/electron-packager.js . --platform=darwin --out=./out",
    "publish:win": "./node_modules/electron-packager/bin/electron-packager.js . --platform=win32 --out=./out",
    "publish:linux": "./node_modules/electron-packager/bin/electron-packager.js . --platform=linux --out=./out"
  },
  "keywords": [],
  "author": {
    "name": "MMichotte",
    "email": "martin.michotte@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron-test"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "11.1.1",
    "electron-packager": "^15.2.0"
  }
}
